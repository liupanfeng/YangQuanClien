        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="e10bd3d89e57e45c4d5ea62927d4e022" />
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="5df9ccef0cafb2fc960004a7" />
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="DEFAULT_CHANNEL" /> <!-- 极光推送 -->
        <!-- Required SDK核心功能 -->
        <meta-data
            android:name="JPUSH_CHANNEL"
            android:value="developer-default" /> <!-- Required SDK 核心功能 -->
        <!-- 可配置android:process参数将PushService放在其他进程中 -->
        <meta-data
            android:name="JPUSH_APPKEY"
            android:value="16e4843c6591302c5d1b26de" /> <!-- since 3.0.9 Required SDK 核心功能 -->
        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.tangyang.yangquan.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
