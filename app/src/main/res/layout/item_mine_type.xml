<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/ll_mine_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp33"
        android:layout_centerInParent="true"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/iv_mine_type"
            android:layout_width="23dp"
            android:layout_height="22dp"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="24dp"
            android:background="@mipmap/ic_mine_my_collection" />

        <TextView
            android:id="@+id/tv_mine_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@id/iv_mine_type"
            android:layout_marginTop="@dimen/dp11"
            android:text="完善资料"
            android:textColor="#333333"
            android:textSize="15sp" />


    </RelativeLayout>


</RelativeLayout>