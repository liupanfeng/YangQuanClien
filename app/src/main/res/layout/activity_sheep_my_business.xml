<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.SheepMyBusinessActivity">

    <include layout="@layout/custom_title"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp8"
        android:background="#fff5f5f5"
        />

    <LinearLayout
        android:id="@+id/ll_no_message"
        android:orientation="vertical"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="111dp"
            android:layout_height="23dp"
            android:text="无业务信息！"
            android:textColor="#ff090723"
            android:textSize="17sp"
            android:layout_marginTop="@dimen/dp70"
            android:layout_gravity="center_horizontal"
            />

        <Button
            android:id="@+id/btn_apply"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp43"
            android:background="@drawable/btn_apply_service_bg"
            android:text="立即申请"
            android:textColor="@color/white"
            android:layout_marginLeft="@dimen/dp40"
            android:layout_marginRight="@dimen/dp40"
            android:layout_marginTop="@dimen/dp50"
            />

        <Button
            android:id="@+id/btn_back"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp43"
            android:background="@drawable/btn_apply_service_back_bg"
            android:text="返回"
            android:textColor="@color/mainColor_yellow"
            android:layout_marginLeft="@dimen/dp40"
            android:layout_marginRight="@dimen/dp40"
            android:layout_marginTop="@dimen/dp20"
            />
    </LinearLayout>


    <RelativeLayout
        android:id="@+id/rl_my_business_review"
        android:orientation="vertical"
        android:background="@drawable/bg_sheep_my_business"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginRight="@dimen/dp15"
        android:layout_marginTop="@dimen/dp8"
        android:layout_width="match_parent"
        android:layout_height="166dp">

        <ImageView
            android:layout_width="@dimen/dp260"
            android:layout_height="@dimen/dp65"
            android:background="@mipmap/ic_my_business_review"
            android:layout_marginTop="@dimen/dp38"
            android:layout_centerHorizontal="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提示"
            android:textColor="#ffc2c2c2"
            android:textSize="17sp"
            android:layout_marginTop="123dp"
            android:layout_centerHorizontal="true"
            />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_my_business_apply"
        android:orientation="vertical"
        android:background="@drawable/bg_sheep_my_business"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginRight="@dimen/dp15"
        android:layout_marginTop="@dimen/dp8"
        android:layout_width="match_parent"
        android:layout_height="166dp">

        <ImageView
            android:layout_width="@dimen/dp260"
            android:layout_height="@dimen/dp65"
            android:background="@mipmap/ic_my_business_apply"
            android:layout_marginTop="@dimen/dp38"
            android:layout_centerHorizontal="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提示"
            android:textColor="#ffc2c2c2"
            android:textSize="17sp"
            android:layout_marginTop="123dp"
            android:layout_centerHorizontal="true"
            />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl_my_business_first"
        android:orientation="vertical"
        android:background="@drawable/bg_sheep_my_business"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginRight="@dimen/dp15"
        android:layout_marginTop="@dimen/dp8"
        android:layout_width="match_parent"
        android:layout_height="166dp">

        <ImageView
            android:layout_width="@dimen/dp260"
            android:layout_height="@dimen/dp65"
            android:background="@mipmap/ic_my_business_first"
            android:layout_marginTop="@dimen/dp38"
            android:layout_centerHorizontal="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提示"
            android:textColor="#ffc2c2c2"
            android:textSize="17sp"
            android:layout_marginTop="123dp"
            android:layout_centerHorizontal="true"
            />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl_my_business_finish"
        android:orientation="vertical"
        android:background="@drawable/bg_sheep_my_business"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginRight="@dimen/dp15"
        android:layout_marginTop="@dimen/dp8"
        android:layout_width="match_parent"
        android:layout_height="166dp">

        <ImageView
            android:layout_width="@dimen/dp260"
            android:layout_height="@dimen/dp65"
            android:background="@mipmap/ic_my_business_finish"
            android:layout_marginTop="@dimen/dp38"
            android:layout_centerHorizontal="true"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提示"
            android:textColor="#ffc2c2c2"
            android:textSize="17sp"
            android:layout_marginTop="123dp"
            android:layout_centerHorizontal="true"
            />

    </RelativeLayout>



</LinearLayout>