<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.SheepBreedServerActivity">

    <include layout="@layout/custom_title"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp8"
        android:background="#fff5f5f5"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="养殖应用"
        android:textColor="#ff656565"
        android:textSize="13sp"
        android:layout_marginTop="@dimen/dp13"
        android:layout_marginLeft="@dimen/dp14"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp17"
        android:layout_marginLeft="@dimen/dp14"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/ll_apply_service"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <ImageView
                android:layout_width="@dimen/dp43"
                android:layout_height="@dimen/dp43"
                android:background="@mipmap/ic_apply_service"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="申请服务"
                android:textColor="#ff656565"
                android:layout_marginTop="@dimen/dp5"
                android:textSize="12sp"
                />

        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_my_business"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp29"
            android:orientation="vertical"
            >

            <ImageView
                android:layout_width="@dimen/dp43"
                android:layout_height="@dimen/dp43"
                android:background="@mipmap/ic_my_service"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的业务"
                android:textColor="#ff656565"
                android:layout_marginTop="@dimen/dp5"
                android:textSize="12sp"
                />

        </LinearLayout>


    </LinearLayout>

</LinearLayout>