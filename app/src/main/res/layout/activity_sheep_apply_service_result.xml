<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.SheepApplyServiceActivity">

    <include layout="@layout/custom_title"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp8"
        android:background="#fff5f5f5"
        />

    <ImageView
        android:layout_width="84dp"
        android:layout_height="84dp"
        android:background="@mipmap/ic_apply_service_result"
        android:layout_marginTop="@dimen/dp60"
        android:layout_gravity="center_horizontal"
        android:scaleType="fitXY"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="已提交，请耐心等待～"
        android:textColor="#ff090723"
        android:textSize="17sp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp16"
        />


    <Button
        android:id="@+id/btn_my_business"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp43"
        android:background="@drawable/btn_apply_service_bg"
        android:text="我的业务"
        android:textColor="@color/white"
        android:layout_marginLeft="@dimen/dp40"
        android:layout_marginRight="@dimen/dp40"
        android:layout_marginTop="@dimen/dp50"
        />


    <Button
        android:id="@+id/btn_back"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp43"
        android:background="@drawable/btn_apply_service_back_bg"
        android:text="返回"
        android:textColor="@color/mainColor_yellow"
        android:layout_marginLeft="@dimen/dp40"
        android:layout_marginRight="@dimen/dp40"
        android:layout_marginTop="@dimen/dp20"
        />


</LinearLayout>